<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="256.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="computermanager.controller.LoadViewController">
   <children>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name " />
            <TableColumn prefWidth="75.0" text="Price" />
         </columns>
      </TableView>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button fx:id="LoadButton" mnemonicParsing="false" onMouseClicked="#loadButtonClicked" text="Load" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onMouseClicked="#cancelButtonClicked" text="Cancel" />
            <Button fx:id="deleteButton" mnemonicParsing="false" onMouseClicked="#deleteButtonClicked" text="Delete" />
         </children>
      </HBox>
   </children>
</VBox>
